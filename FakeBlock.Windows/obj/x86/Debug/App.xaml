<Application
    x:Class="FakeBlock.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:FakeBlock.ViewModels">
    <Application.Resources>
        <ResourceDictionary>
            <local:WoodGrain x:Key="CurrentGrain" Name = "Zebra wood" ImageFilename = "Assets/Wood/zebrawood.jpg" BlockSoundFilename = "ms-appx:///Assets/Sounds/woodblock1.wav" />
            <local:BlockViewModel x:Key="BlockVM" SelectedGrain="{StaticResource CurrentGrain}"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>

